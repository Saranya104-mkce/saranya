a=list(map(str,input().split(" ")))
b=list(map(str,input().split(" ")))
c=list(map(str,input().split(" ")))
#print(a)
#print(b)
#print(c)
d=list(a[0]+b[0]+c[0])
e=sorted(d)
f=list(a[1]+b[1]+c[1])
g=sorted(f)
h=list(a[2]+b[2]+c[2])
i=sorted(h)
j=list(a[3]+b[3]+c[3])
k=sorted(j)
#print(e)
#print(g)
#print(i)
#print(k)
l=list(e[0]+g[0]+i[0]+k[0])
print(l)
mmm=e[0]+g[0]+i[0]+k[0]
print(mmm)
x1=int(e[0])
x2=int(g[0])
x3=int(i[0])
x4=int(k[0])
y=x1+x2+x3+x4
print(y)
